items ansible_host=10.0.2.221 ansible_user=ubuntu ansible_ssh_private_key_file=../key.pem  docker_image=pablop115/items name=items ports=3003:3003

[loadbalancers]
loadbalancer dns=restaurant-loadbalancer-859168787.eu-north-1.elb.amazonaws.com

[web_servers:vars]
ansible_ssh_common_args='-o ProxyCommand="ssh -i ../key.pem -W %h:22 ubuntu@13.60.8.71" -o ConnectTimeout=3000 -o StrictHostKeyChecking=no'
